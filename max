function checkBrackets(str) {
    const stack = [];
    const pairs = { ')': '(', ']': '[', '}': '{' };
  
    for (let i = 0; i < str.length; i++) {
      const char = str[i];
      if (char === '(' || char === '[' || char === '{') {
        stack.push(char);
      } else if (char === ')' || char === ']' || char === '}') {
        const last = stack.pop();
        if (last !== pairs[char]) {
          return false;
        }
      }
    }
  
    return stack.length === 0;
  }

console.log(checkBrackets("hello (world my (name) is alibi")); 
